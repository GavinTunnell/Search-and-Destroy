<?xml version="1.0" encoding="UTF-8"?>
<root main_tree_to_execute="MainTree">

  <BehaviorTree ID="MainTree">
    <Sequence name="navigate_sequence">
      <ComputePathToPose
        name="compute_path_to_pose"
        goal="{goal}"
        path="{path}"
        planner_id="GridBased"/>

      <FollowPath
        name="follow_path"
        path="{path}"
        controller_id="FollowPath"/>
    </Sequence>
  </BehaviorTree>

</root>
